{
  "name": "allianceai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "python:install": "cd backend-deploy && pip install -r requirements.txt",
    "python:server": "cd backend-deploy && python run_server.py",
    "python:dev": "cd backend-deploy && cd alliance-backend && python3 -m uvicorn server:app --reload --host 0.0.0.0 --port 8000",
    "dev:all": "concurrently \"npm run dev\" --names \"frontend,backend\" --prefix-colors \"blue,green\""
  },
  "dependencies": {
    "@apollo/client": "^4.0.9",
    "@apollo/server": "^4.12.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.4",
    "graphql": "^16.12.0",
    "graphql-tag": "^2.12.6",
    "mapbox-gl": "^3.16.0",
    "maplibre-gl": "^5.10.0",
    "mongodb": "^6.20.0",
    "next": "16.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-map-gl": "^8.1.0",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
